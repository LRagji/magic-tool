{
    "project": {
        "logs": "console",
        "type": "angular8",
        "workspace":"../workspace/",
        "name":"test",
        "commands": [
            {
                "cmd": "npx",
                "keepRunningInBackgroud": false,
                "args": [
                    "ng",
                    "--version"
                ],
                "options": {
                    "cwd": "../workspace/"
                }
            },
            {
                "cmd": "npx",
                "keepRunningInBackgroud": false,
                "args": [
                    "ng",
                    "new",
                    "test",
                    "--commit=false",
                    "--interactive=false",
                    "--skipInstall=true",
                    "--skipGit=true",
                    "--routing=true",
                    "--style=css",
                    "--verbose=true"
                ],
                "options": {
                    "cwd": "../workspace/"
                }
            },
            {
                "cmd": "npm",
                "keepRunningInBackgroud": false,
                "args": [
                    "install",
                    "--verbose"
                ],
                "options": {
                    "cwd": "../workspace/test/"
                }
            },
            {
                "cmd": "node",
                "keepRunningInBackgroud": false,
                "args": [
                    "layout-generator",
                    "magic-tool.json"
                ]
            }
        ],
        "modules": [
            {
                "name":"alert",
                "bootstrapComponent": "main",
                "components": [
                    {
                        "name": "main",
                        "container": "fluid",
                        "layouts": [
                            {
                                "type": "row",
                                "elements": [
                                    {
                                        "width":1,
                                        "package":"ngx-bootstrap",
                                        "name":"buttons"
                                    }
                                ]
                            }
                        ]
                    }
                ],
                "services": [],
                "pipes": []
            }
        ]
    }
}