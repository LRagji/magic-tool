{
    "$schema": "http://json-schema.org/schema",
    "id": "add-styles",
    "title": "Adds custom css to your projects",
    "type": "object",
    "properties": {
      "project": {
        "type": "string",
        "description": "Project name",
        "x-prompt": "Enter the project name:"
      },
      "stylePath": {
        "type": "string",
        "description": "Stylesheet path to import",
        "x-prompt": "Enter path for style sheet:"
      }
    },
    "required": ["stylePath","project"]
  }