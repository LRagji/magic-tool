{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "add-imports": {
      "description": "Adds import in a angular module.",
      "factory": "./add-imports/index#addImports",
      "schema": "./add-imports/schema.json"
    },
    "add-declare-imports": {
      "description": "Adds js import in a file.",
      "factory": "./add-declare-imports/index#addDecalreImports",
      "schema": "./add-declare-imports/schema.json"
    },
    "add-entry-routing-table": {
      "description": "Adds routing entry in a router module.",
      "factory": "./add-entry-routing-table/index#addRoutingEntry",
      "schema": "./add-entry-routing-table/schema.json"
    }
  }
}
