    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Service",
            "program": "${workspaceFolder}/service/service.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Element Repo",
            "program": "${workspaceFolder}/element-repository/element-repository.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Magic Tool",
            "program": "${workspaceFolder}/transpiler/entry-point.js"
        }
    ]
}